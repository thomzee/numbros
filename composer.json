    "description": "Generate invoice number, order code, or receipt number easily with dynamic format and pattern for PHP Laravel framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Andarianto",
            "email": "thomaskayu@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/support": "5.x|6.x|7.x|8.x"
    },
    "autoload": {
        "psr-4": {
            "Thomzee\\Numbros\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thomzee\\Numbros\\NumbrosServiceProvider"
            ]
        }
    }
}
